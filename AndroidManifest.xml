<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.deceptacle.spartacus"
      android:installLocation="auto" android:versionCode="2" android:versionName="0.30">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="SpartacusApplication" android:theme="@android:style/Theme.Light" android:debuggable="true">
        <activity android:name=".StartupActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="WorkoutActivity"></activity>
        <activity android:name="RestActivity"></activity>
        <activity android:name="CompletedActivity"></activity>

    </application>
</manifest>